{
  "name": "get-client-ip",
  "version": "0.1.1",
  "description": "Get Client IP Address",
  "scripts": {
    "dev": "nodemon ./api/index.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run lint && npm run clean",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json && touch dist/esm/package.json && echo {\"type\": \"module\"} > dist/esm/package.json",
    "build:cjs": "tsc -p tsconfig.cjs.json && touch dist/cjs/package.json && echo {\"type\": \"commonjs\"} > dist/cjs/package.json",
    "lint": "eslint --ext .ts src/",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:all": "prettier --write \"**/*.{ts,tsx,js,jsx,cjs,mjs,json}\"",
    "check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "pretest": "npm run lint && npm run check",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build"
  },
  "authors": [
    "WolfieLeader",
    "Ben Halverson"
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.4",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "express": "^4.18.2",
    "jest": "^29.1.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "keywords": []
}
